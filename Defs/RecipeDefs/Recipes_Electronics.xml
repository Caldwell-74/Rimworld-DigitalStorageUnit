<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- chip I -->
    <RecipeDef ParentName="DSU_BaseRecipe_Electronics">
        <defName>Make_DSU_ElectronicChip_I</defName>
        <label>make basic AI chip</label>
        <description>Make basic AI chip</description>
        <jobString>Making basic AI chip</jobString>
        <workAmount>2000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentIndustrial</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentIndustrial</li>
                <li>Steel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <PRF_ElectronicChip_I>1</PRF_ElectronicChip_I>
        </products>
        <skillRequirements>
            <Crafting>8</Crafting>
        </skillRequirements>
    </RecipeDef>

    <!-- chip II -->
    <RecipeDef ParentName="DSU_BaseRecipe_Electronics">
        <defName>Make_DSU_ElectronicChip_II</defName>
        <label>make advanced AI core</label>
        <description>Make an advanced AI core</description>
        <jobString>Making Adv. AI core</jobString>
        <workAmount>4000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>PRF_ElectronicChip_I</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentSpacer</li>
                <li>Steel</li>
                <li>PRF_ElectronicChip_I</li>
                <li>Plasteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <PRF_ElectronicChip_II>1</PRF_ElectronicChip_II>
        </products>
        <skillRequirements>
            <Crafting>15</Crafting>
        </skillRequirements>
    </RecipeDef>

    <!-- chip III -->
    <RecipeDef ParentName="DSU_BaseRecipe_Electronics">
        <defName>Make_DSU_ElectronicChip_III</defName>
        <label>make qubit processor</label>
        <description>Make a qubit processor</description>
        <jobString>Making qubit processor</jobString>
        <workAmount>10000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Gold</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>PRF_ElectronicChip_II</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>100</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentSpacer</li>
                <li>Gold</li>
                <li>PRF_ElectronicChip_II</li>
                <li>Plasteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <PRF_ElectronicChip_III>1</PRF_ElectronicChip_III>
        </products>
        <skillRequirements>
            <Crafting>18</Crafting>
        </skillRequirements>
    </RecipeDef>
</Defs>
